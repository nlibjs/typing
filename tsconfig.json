{
	"compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"removeComments": true,
		"declaration": true,
		"sourceMap": false,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"allowImportingTsExtensions": true,
		"emitDeclarationOnly": true,
		"outDir": "./types"
	},
	"include": ["./src/**/*.ts"]
}
